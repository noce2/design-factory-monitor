{
  "name": "design-factory-monitor",
  "version": "0.0.0",
  "description": "Application allowing you monitor your design factory",
  "private": true,
  "engines": {
    "node": "^6.11.1"
  },
  "main": "bin/www",
  "scripts": {
    "build:insight360App": "rimraf ./public/insight360App && webpack --config ./config/webpack.prod.js --progress --profile --bail",
    "devstart": "set DEBUG=design-factory-monitor:* && node ./bin/www",
    "devstart:watch": "nodemon --exec \"npm run devstart\"",
    "devstart:insight360App": "webpack-dev-server --inline --progress --port 8081 --config ./config/webpack.dev.js",
    "test:insight360App": "karma start ./karma.conf.js --auto-watch",
    "test:watch": "nodemon --exec \"npm test\"",
    "test": "mocha ./tests/*.test.js",
    "pree2e": "webdriver-manager update",
    "e2e:insight360App": "protractor ./protractor.config.js",
    "prestart": "npm run build:insight360App",
    "start": "set DEBUG=design-factory-monitor:* && node ./bin/www"
  },
  "dependencies": {
    "@angular/animations": "^4.4.0-RC.0",
    "@angular/common": "^4.4.0-RC.0",
    "@angular/compiler": "^4.4.0-RC.0",
    "@angular/compiler-cli": "^4.4.0-RC.0",
    "@angular/core": "^4.4.0-RC.0",
    "@angular/forms": "^4.4.0-RC.0",
    "@angular/http": "^4.4.0-RC.0",
    "@angular/platform-browser": "^4.4.0-RC.0",
    "@angular/platform-browser-dynamic": "^4.4.0-RC.0",
    "@angular/platform-server": "^4.4.0-RC.0",
    "@angular/router": "^4.4.0-RC.0",
    "@webcomponents/custom-elements": "^1.0.0",
    "body-parser": "~1.17.1",
    "clarity-angular": "^0.10.2",
    "clarity-icons": "^0.10.2",
    "clarity-ui": "^0.10.2",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.1",
    "debug": "~2.6.3",
    "devextreme": "^17.1.5",
    "devextreme-angular": "^17.1.5",
    "express": "~4.15.2",
    "morgan": "~1.8.1",
    "mutationobserver-shim": "^0.3.2",
    "pug": "~2.0.0-beta11",
    "rxjs": "^5.4.3",
    "serve-favicon": "~2.4.2",
    "web-animations-js": "^2.2.1",
    "zone.js": "^0.8.4"

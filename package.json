{
  "name": "design-factory-monitor",
  "version": "0.0.0",
  "description": "Application allowing you monitor your design factory",
  "private": true,
  "engines": {
    "node": "^6.11.1"
  },
  "main": "bin/www",
  "scripts": {
    "devstart": "set DEBUG=design-factory-monitor:* && node ./bin/www",
    "devstart:watch": "nodemon --exec \"npm run devstart\"",
    "test:watch": "nodemon --exec \"npm test\"",
    "test": "mocha ./tests/*.test.js"
  },
  "dependencies": {
    "@webcomponents/custom-elements": "^1.0.0",
    "body-parser": "~1.17.1",
    "clarity-icons": "^0.10.0-alpha.6",
    "clarity-ui": "^0.10.0-alpha.6",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "morgan": "~1.8.1",
    "pug": "~2.0.0-beta11",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "expect.js": "^0.3.1",
    "mocha": "^3.5.0",
    "supertest": "^3.0.0"
  }
}
